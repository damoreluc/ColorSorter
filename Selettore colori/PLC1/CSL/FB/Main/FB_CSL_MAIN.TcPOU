<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="FB_CSL_MAIN" Id="{f7c0fb49-0a9a-4d93-8227-9668f0bcf9c2}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_CSL_MAIN
VAR_INPUT
	bEnable		:	BOOL;
END_VAR
VAR_IN_OUT
	stIn		:	ST_CSL_In;
	stParams	: 	ST_CSL_Params;
	stOut		:	ST_CSL_Out;
END_VAR
VAR_OUTPUT
	eStep	: E_Step;	//current step of the state machine
	eSubStepSensor: E_Step;
	eSubStepPistons: E_Step;
	eErrStep : E_Step;  //step where an error was raised
	eErrSubStep: E_Step;
	nErrid	: DINT;		//error identification code
	tStepTime : TIME;
	nErrSubid	: DINT;
	
	bFbEnd	: BOOL;
	
	nTotalRevs : REAL;
	
	nCycles : INT;
END_VAR
VAR
	bInitialized	: BOOL;		//FB initialization
	eNewStep : E_Step;			//request for a new step
	bEntryact	: 	BOOL;		//step entry action performed
	fbTonStep :  TON;			// elapsed time on each state machine step
	
	fbSensor	: 	FB_MSF_Sensor;
	fbPiston	:	FB_MSF_Pistons;
	
	fbOscillator:	FB_Oscillator;
	
	fbRestore	:	FB_Restore;
	
	bFbSensorEnd: BOOL;
	bFbPistonEnd: BOOL;
	
	
	nSlot : INT;
	
	Timer_bPause : TON();
	bPause: BOOL	:= FALSE;
	
	bConveyorFree : BOOL;
	iMotorPrev: INT;
	iMotorRestore : INT;
	eEmergStep: E_Step;
	
	bFbSensorReset: BOOL;
	bFbPistonsReset: BOOL;
	
	fbRtMotorImpulse: R_TRIG;
	iMotorTotal : INT;
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[(*Initialization of the FB_Sensor*)
IF NOT bInitialized THEN
	eNewStep := E_Step.NoStep;
	eStep := E_Step.MainStop;
	bEntryAct := FALSE;
	bFbEnd := FALSE;
	bInitialized := TRUE;
	bFbSensorReset := FALSE;
	bFbPistonsReset := FALSE;
ELSE
	
	(*Preamble*)
	
	(*Forcing the Not Enabled State*)
	IF NOT bEnable THEN
		eNewStep := E_Step.NeOrError;
	END_IF	
	
	(*Forcing ther emergency state*)
	IF NOT stIn.bEmerg THEN
		IF eStep <> E_Step.MainEmergency THEN
			eEmergStep := eStep;
		END_IF
		eNewStep := E_Step.MainEmergency;
	END_IF
	
	
	(*State change management*)
	IF eNewStep <> E_Step.NoStep THEN
		eStep := eNewStep;
		eNewStep := E_Step.NoStep;
		bEntryact := FALSE;
	END_IF
	
	//bPause management
	(*
	Timer_bPause(IN := NOT stIn.bStop, PT := stParams.stopLongPressTime);
	IF Timer_bPause.Q AND (eStep = E_Step.MainPiston OR eStep = E_Step.MainSensor) THEN
		IF eStep = E_Step.MainSensor AND eSubStepSensor = E_Step.Measuring THEN
			bPause := TRUE;
		ELSE
			eNewStep := E_Step.MainPause;
		END_IF
	END_IF
	*)
	IF NOT stIn.bStop THEN
		bPause := TRUE;
		IF eSubStepSensor = E_Step.Start THEN
			eNewStep := E_Step.MainStop;
		END_IF
	END_IF
	
	fbOscillator(enable := TRUE, t_high := T#500MS, t_low := T#500MS);
	
	//calcolo giri del motori complessivo
	fbRtMotorImpulse(CLK:=	stIn.bMotorImpulse );
	IF fbRtMotorImpulse.Q THEN
		nTotalRevs := nTotalRevs + (1.0 / To_REAL(stParams.StepPerRev));
	END_IF
	
	(* msf subordinate *)
	fbSensor(
		bInitializedExt := bFbSensorReset,
		stIn:= stIn, 
		bEnable:= bEnable,
		bRun:= (eStep = E_Step.MainSensor), 
		eStep=> eSubStepSensor, 
		eErrStep=> eErrSubStep, 
		nErrId=> nErrSubId,
		bFbEnd=> bFbSensorEnd, 
		stOut:= stOut, 
		nSlot=> nSlot, 
		stParams:= stParams
	);
	
	
	fbPiston(
		bInitializedExt := bFbPistonsReset,
		bEnable:= bEnable, 
		slot:= nSlot, 
		stIn:= stIn, 
		bRun := (eStep = E_Step.MainPiston),
		stParams:= stParams, 
		stOut:= stOut, 
		eStep=> eSubStepPistons, 
		eErrStep=> eErrSubStep, 
		nErrid=> nErrSubId, 
		iMotorPrev => iMotorPrev,
		bFbEnd=> bFbPistonEnd,
		nCycles=>nCycles
	);
	
	bFbSensorReset := TRUE;
	bFbPistonsReset := TRUE;
	
	(*Case*)
	CASE eStep OF
		
		(*MainStop Step - S0*)
		E_Step.MainStop :
			(*Body*)
			bPause := FALSE;
			
			stOut.bLStart	:= fbOscillator.q;
			stOut.bLStop := TRUE;
			stOut.bLAck := FALSE;
			stOut.bMotor:=FALSE;
		
			(*Transition - to MainSensor*)
			IF stIn.bStart AND bEnable THEN
				eNewStep := E_Step.MainSensor;
			END_IF
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
			END_IF
			
		(*MainSensor Step - S1*)
		E_Step.MainSensor :
			(*Body*)
			stOut.bLStart	:= TRUE;
			stOut.bMotor := fbSensor.bMotor OR fbPiston.bMotor;
			IF bPause THEN
				stOut.bLStop := fbOscillator.q;
			ELSE
				stOut.bLStop := FALSE;
			END_IF
			stOut.bLAck := FALSE;
		(*
			fbSensor(
				bInitializedExt := NOT bFbSensorReset,
				stIn:= stIn, 
				bEnable:= bEnable, 
				eStep=> eSubStep, 
				eErrStep=> eErrSubStep, 
				nErrId=> nErrSubId,
				bFbEnd=> bFbSensorEnd, 
				stOut:= stOut, 
				nSlot=> nSlot, 
				stParams:= stParams
			);
			*)
			
			//bFbSensorReset := FALSE;
			
			(*Transition - to Processing*)
			IF bFbSensorEnd AND bEnable THEN
				eNewStep := E_Step.MainPiston;
			END_IF
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
				
				bFbSensorReset := FALSE;
			END_IF
		
		(*MainPiston Step - S2*)
		E_Step.MainPiston :
			(*Body*)
			stOut.bLStart	:= TRUE;
			stOut.bMotor := fbSensor.bMotor OR fbPiston.bMotor;
			IF bPause THEN
				stOut.bLStop := fbOscillator.q;
			ELSE
				stOut.bLStop := FALSE;
			END_IF
			stOut.bLAck := FALSE;
		
			
(*
			fbPiston(
				bInitializedExt := NOT bFbPistonsReset,
				bEnable:= bEnable, 
				slot:= nSlot, 
				stIn:= stIn, 
				stParams:= stParams, 
				stOut:= stOut, 
				eStep=> eSubStep, 
				eErrStep=> eErrSubStep, 
				nErrid=> nErrSubId, 
				iMotorPrev => iMotorPrev,
				bFbEnd=> bFbPistonEnd
			);
			*)
			
			//bFbPistonsReset := FALSE;
			
			(*Transition - to MainSensor*)
			(*Transition - to MainPause*)
			IF bFbPistonEnd AND bEnable THEN
				IF bPause THEN
					eNewStep := E_Step.MainStop; //to MainStop
				ELSE
					eNewStep := E_Step.MainSensor; //to MainSensor
				END_IF	
			END_IF
			
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
			END_IF	
			
		(*MainPause Step - S3*)
		E_Step.MainPause :
			(*Body*)
			stOut.bLStart	:= FALSE;
			stOut.bLStop := TRUE;
			stOut.bLAck := FALSE;
			stOut.bMotor:=FALSE;
			
			(*Transition - to MainSensor*)
			IF stIn.bStart AND bEnable THEN
				eNewStep := E_Step.MainSensor;
				bPause := FALSE;
			END_IF
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
			END_IF	
		
		(*MainEmergency Step - S4*)
		E_Step.MainEmergency :	
			(*Body*)
			stOut.bLStart	:= FALSE;
			stOut.bLStop := FALSE;
			stOut.bLAck := fbOscillator.q;
			stOut.bMotor:=FALSE;
			
			//da vedere il pilotaggio delle uscite: motore, compressore e valvole
			stOut.bMotor := FALSE;
			stOut.bCompressor := FALSE;
		
			(*Transition - to Measuring*)
			IF stIn.bAck AND bEnable THEN
				eNewStep := E_Step.MainAcknowledge;
			END_IF
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				iMotorRestore := iMotorPrev;
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
			END_IF
			
		(*MainAcknowledge Step - S5*)	
		E_Step.MainAcknowledge :		
			(*Body*)
			stOut.bLStart	:= FALSE;
			stOut.bLStop := FALSE;
			stOut.bLAck := TRUE;
			
			//fb per liberare il nastro
			fbRestore(
				bEnable:= TRUE, 
				iMotorPrev:= iMotorPrev, 
				eStep:= eEmergStep, 
				eSubStepSensor:= eSubStepSensor,
				eSubStepPistons:= eSubStepPistons,
				stIn:= stIn, 
				stParams:= stParams, 
				stOut:= stOut, 
				bFbEnd=> bConveyorFree
			);
		
			(*Transition - to Measuring*)
			IF bConveyorFree AND bEnable THEN
				bInitialized := FALSE;
				iMotorPrev := 0;
			END_IF
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
			END_IF
		
		(*NeOrError step*)	
		E_Step.NeOrError :
			(*Body*)
			stOut.bLStart	:= fbOscillator.q;
			stOut.bLStop := fbOscillator.q;
			stOut.bLAck := fbOscillator.q;
			
			stOut.bMotor := FALSE;
		
			(*Transition - to Measuring*)
			
			
			(*Exit Act*)
			IF eNewStep <> E_Step.NoStep THEN
				IF eNewStep = E_Step.NeOrError THEN
					eErrStep := eStep;
					//nErrId	:= 16#9999_9999;
				END_IF
			END_IF
			
	END_CASE
	
		(* elapsed time into the current step *)
	fbTonStep(IN := (eNewstep = E_Step.NoStep), PT := T#500H);
	tStepTime := fbTonStep.ET;
END_IF]]></ST>
